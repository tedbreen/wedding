{
  "name": "wedding",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "npm run watch-server & npm run watch-less",
    "watch-server": "nodemon --watch ./ --ext hbs,js,json ./bin/www",
    "watch-less": "nodemon --watch ./public/stylesheets --ext less --exec 'npm run build-less'",
    "build-less": "lessc --source-map='./public/stylesheets/bundle.css.map' ./public/stylesheets/layout.less ./public/stylesheets/bundle.css"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "compression": "^1.6.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "hbs": "~3.1.0",
    "less": "^2.6.1",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "eslint": "^2.4.0",
    "nodemon": "^1.9.1"
  }
}
